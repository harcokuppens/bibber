@INPROCEEDINGS{EE-2012-Wilmont-RoleAbstraction,
  author = {Wilmont, Ilona and Barendsen, Erik and Hoppenbrouwers, Stijn},
  title = {On the Role of Abstraction and Executive Control in Process Modeling},
  booktitle = {Proceedings of The Practice of Enterprise Modeling, 5th IFIP WG8.1
