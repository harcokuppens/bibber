@INPROCEEDINGS{id6,
  author = {Basten, Twan and van Benthum, Emiel and Geilen, Marc and Hendriks,
	Martijn and Houben, Fred and Igna, Georgeta and Reckers, Frans and
	de Smet, Sebastian and Somers, Lou and Teeselink, Egbert and TrÄ�ka,
